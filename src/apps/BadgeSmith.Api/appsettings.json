{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    }
  },
  "LoggingDestinations": {
    "ConsoleLogging": true,
    "EnableJsonLogging": true,
    "OpenTelemetry": false
  },
  "AllowedHosts": "*",
  "LocalStack": {
    "UseLocalStack": false
  }
}
